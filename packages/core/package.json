{
  "name": "@css-gardener/core",
  "version": "0.0.2",
  "description": "The core functionality of the css-gardener tool",
  "main": "index.js",
  "bin": {
    "css-gardener": "index.js"
  },
  "files": [
    "src",
    "dist",
    "deps.edn",
    "index.js",
    "shadow-cljs.edn"
  ],
  "scripts": {
    "lint": "clojure -A:lint -m clj-kondo.main --lint src --cache false",
    "repl": "node target/main-dev/main.js",
    "release": "shadow-cljs release main",
    "test-watch": "shadow-cljs watch main-test",
    "compile-tests": "shadow-cljs release main-test",
    "run-tests": "node target/main-test/main.js",
    "test": "npm run compile-tests && npm run run-tests",
    "deploy": "npm run release && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dking1286/css-gardener/packages/core"
  },
  "keywords": [
    "css",
    "cljs",
    "clojure",
    "clojurescript",
    "stylesheet",
    "style"
  ],
  "author": "Daniel King",
  "license": "EPL-2.0",
  "dependencies": {
    "chokidar": "3.4.0"
  },
  "devDependencies": {
    "shadow-cljs": "2.9.3"
  }
}