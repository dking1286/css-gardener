{
  "name": "generator-css-gardener",
  "version": "0.0.2",
  "description": "Generator for CLJS project boilerplate using css-gardener",
  "homepage": "",
  "author": {
    "name": "Daniel King",
    "email": "daniel.oliver.king@gmail.com"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "clojure",
    "clojurescript",
    "css",
    "cljs",
    "stylesheet",
    "style",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "1.7.0",
    "yeoman-assert": "3.1.0",
    "eslint": "6.6.0",
    "prettier": "1.19.1",
    "husky": "3.0.9",
    "lint-staged": "9.4.3",
    "eslint-config-prettier": "6.6.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-config-xo": "0.27.2",
    "jest": "26.1.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "lodash.camelcase": "4.3.0",
    "lodash.kebabcase": "4.1.1",
    "lodash.snakecase": "4.1.1",
    "yeoman-generator": "2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "compile-tests": "echo 'No compilation required for tests'",
    "run-tests": "jest",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dking1286/css-gardener/packages/generator-css-gardener"
  },
  "license": "EPL-2.0"
}
